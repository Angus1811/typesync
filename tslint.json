{
  "extends": [
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true, "single"
    ],
    "no-var-requires": false,
    "trailing-comma": [false],
    "ordered-imports": [false],
    "no-console": [
      false
    ],
    "member-access": [false],
    "object-literal-sort-keys": false
  }
}
